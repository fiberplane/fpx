{
  "$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
  "extends": ["../biome.jsonc"],
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "correctness": {
        "noUnusedImports": {
          "level": "error"
        },
        "noUnusedVariables": {
          "level": "error"
        }
      }
    }
  },
  "files": {
    "ignore": ["meta/*.json", "dist/*.*", "test-content/*.*", "seed-assets/*.json"]
  }
}
